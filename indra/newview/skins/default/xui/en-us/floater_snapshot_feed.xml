<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<floater
 can_minimize="false"
 title="Post to My Profile Feed"
 height="380"
 width="480"
 layout="topleft"
 name="floater_snapshot_profile"
 rect_control="FloaterSnapshotFeedRect">
    <text
     length="1"
	 follows="top|left"
     top="-180"
     font="SansSerif"
     height="16"
     layout="topleft"
     left="10"
     name="caption_label"
     right="-10"
     type="string">
     Enter text to post below:
    </text>
    <text_editor
     follows="all"
     height="155"
     layout="topleft"
     length="1"
     max_length="700"
     name="caption"
     right="-10"
	 bottom_delta="30"
     type="string"
     word_wrap="true"/>
    <check_box
     follows="left|bottom"
     initial_value="true"
     label="Include location"
     layout="topleft"
     left_delta="0"
     name="add_location_cb"
     bottom_delta="-25" />
    <button
     follows="right|bottom"
     height="23"
     label="Cancel"
     layout="topleft"
     name="cancel_btn"
     right="-10"
     bottom="10"
     width="100"/>
    <button
     follows="right|bottom"
     height="23"
     label="Post"
     layout="topleft"
     left_delta="-106"
     name="post_btn"
     bottom_delta="0"
     width="100"/>
</floater>
