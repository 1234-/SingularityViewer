        visible="false"
        enabled="false"
        border_visible="true"
        bottom_delta="0"
        follows="top|left"
        bottom="-5"
        name="title_media"
        width="4"
        height="4"
        start_url="about:blank"
        decouple_texture_size="true" />
      <button
        follows="right|top"
        height="20"
        label="Align"
        left="166"
        name="button align"
        bottom="-48"
        tool_tip="Align media texture (must load first)"
        width="80" />
